{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
      "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/public/resources/"
    },
    {
      "src": "/favicon/(.*)",
      "dest": "/public/favicon/"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://maharlika-v1.vercel.app",
        "APP_KEY": "base64:D1540pXprDYif14Qrf1LFMjqbgkNieMM5OXYQ9+fZHg=",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
